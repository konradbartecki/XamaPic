<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ImageMerge.Pages.PhotoPage"
             >
 <StackLayout HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
   <StackLayout x:Name="ImageContainer" HorizontalOptions="FillAndExpand">
     <Image x:Name="ThugImage" Aspect="AspectFit" HorizontalOptions="FillAndExpand"/>
     <Image HorizontalOptions="Start" x:Name="Cygaro"  HeightRequest="80" Aspect="AspectFit" Source="cygaro.png"></Image>
     <Image HorizontalOptions="Start" x:Name="Oksy" Aspect="AspectFit" WidthRequest="250" Source="oksy.png" ></Image>
     <Image HorizontalOptions="Start" x:Name="TsTekst" Aspect="AspectFit" WidthRequest="250" Source="tltext.png" ></Image>
   </StackLayout>
 </StackLayout>
</ContentPage>